@model IEnumerable<Towns>
    @{
        Html.DevExpress().TokenBox(settings =>
        {
            settings.Name = "TownIds";
            settings.Properties.Caption = "Towns";
            settings.Width = Unit.Percentage(100);
            settings.CallbackRouteValues = new { Controller = "FileManagement", Action = "TokenBoxTownsPartials", quarryId = ViewBag.QuarryId };
            settings.Properties.TextField = "Name";
            settings.Properties.ValueField = "Id";
            settings.PreRender = (s, e) =>
            {
                var tkb = s as MVCxTokenBox;
                if(Model!=null)
                foreach (var i in Model)
                {
                    tkb.Tokens.Add(i.Name);
                }

            };
        }).BindList(ViewBag.Towns).GetHtml();
    }
